<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mysqlUserDao" class="com.koalain.spring02.dao.implement.MysqlUserDao"></bean>
    <bean id="oracleUserDao" class="com.koalain.spring02.dao.implement.OracleUserDao"></bean>
    <bean id="userService" class="com.koalain.spring02.service.implement.UserServiceImplement">
        <property name="userDao" ref="oracleUserDao"></property>
    </bean>
</beans>